#!/bin/bash
while true
	do
         for i in {1..10}; do
                ./revolve_restarting.sh --experiment-name=evo_cmaes --run=$i --manager=experiments/jlo/learning_cppn_directed.py --n-cores 5 --evaluation-time=50 --learner=cmaes --recovery-enabled=true;
                sleep 5s
	            
	   done
done
